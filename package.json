{
  "name": "pika-web-crawler-test",
  "version": "1.0.0",
  "description": "Web crawler test script",
  "author": "Antonio Marin",
  "license": "MIT",
  "bin": {
    "pika-web-crawler-test": "./bin/run"
  },
  "scripts": {
    "run": "bin/run",
    "run:win": "bin\\run.cmd",
    "posttest": "tsc -p test --noEmit",
    "prepack": "rm -rf lib && tsc",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "prettier": "prettier --parser typescript --single-quote --print-width 120 --write \"./**/*.ts\""
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/cheerio": "^0.22.8",
    "cheerio": "^1.0.0-rc.2",
    "chromeless": "^1.5.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "mocha": "^5",
    "nyc": "^12",
    "ts-node": "^6",
    "typescript": "^2.9",
    "prettier": "^1.12.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "oclif": {
    "bin": "pika-web-crawler-test"
  },
  "types": "lib/index.d.ts"
}
