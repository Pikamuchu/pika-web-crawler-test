{
  "name": "wcrawl",
  "version": "1.0.0",
  "description": "Web crawler test script",
  "bin": {
    "wcrawl": "./bin/run"
  },
  "keywords": [
    "pika",
    "web",
    "crawler",
    "test"
  ],
  "scripts": {
    "start": "bin/run",
    "prepack": "npm run compile",
    "prepare": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "tsc -p test --noEmit",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "prettier": "prettier --parser typescript --single-quote --print-width 120 --write \"./**/*.ts\""
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pikamachu/pika-web-crawler-test"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "author": "Antonio Marin",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "cheerio": "^1.0.0-rc.2",
    "chrome-launcher": "^0.10.2",
    "chromeless": "^1.5.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/cheerio": "^0.22.8",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "codecov": "^3.0.4",
    "mocha": "^5",
    "nyc": "^12.0.2",
    "prettier": "^1.12.0",
    "ts-node": "^6",
    "typescript": "^2.9"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "oclif": {
    "bin": "wcrawl"
  },
  "types": "lib/index.d.ts"
}
